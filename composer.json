{
    "name": "smadeira/planning-center-webhooks",
    "description": "Handle Planning Center webhook calls in a Laravel app",
    "keywords": [
        "planning center",
        "planning-center-webhooks"
    ],
    "homepage": "https://github.com/smadeira/planning-center-webhooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Madeira",
            "email": "smadeira@zoominternet.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/http": "^5",
        "illuminate/routing": "^5",
        "illuminate/support": "^5"

    },
    "autoload": {
        "psr-4": {
            "Smadeira\\PcoWebhooks\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smadeira\\PcoWebhooks\\PcoWebhooksServiceProvider"
            ]
        }
    }
}
